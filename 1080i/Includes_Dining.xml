    <include name="DiningContent">
        <content>
            <!-- NO DATA-->
            <item>
                <visible>!Weather.IsFetched</visible>
                <label>$LOCALIZE[416]</label>
                <thumb>$VAR[WeatherFanartPath]na.jpg</thumb>
                <onclick>noop</onclick>
            </item>
            <!--7 DAYS-->
            <item>
                <visible>Skin.String(WeatherType,)|Skin.String(WeatherType,daily)</visible>
                <visible>Weather.IsFetched + !IsEmpty(Window.Property(Day0.Outlook)) + IsEmpty(Window.Property(Daily.IsFetched))</visible>
                <label>$LOCALIZE[33006]</label>
                <!-- <thumb>$VAR[WeatherFanartPath]$INFO[Window(Weather).Property(Current.FanartCode)].jpg</thumb> -->
