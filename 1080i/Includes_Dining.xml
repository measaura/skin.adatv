                <property name="Icon">$VAR[WeatherIconPath]$INFO[Window(Weather).Property(Current.FanartCode)].png</property>
                <property name="Property1">$INFO[Window.Property(Day0.HighTemp)]$INFO[System.TemperatureUnits]</property>
                <property name="Property2">$INFO[Window.Property(Day0.LowTemp)]$INFO[System.TemperatureUnits]</property>
